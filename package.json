{
  "name": "clone-cli-app",
  "version": "1.0.0",
  "description": "clone base: cli app",
  "repository": {
    "type": "git",
    "ulr": "https://github.com/bizhermit/clone-cli-app.git"
  },
  "bugs": "https://github.com/bizhermit/clone-cli-app/issues",
  "author": "Ryoichi Senda (https://bizhermit.com)",
  "homepage": "https://bizhermit.com",
  "license": "MIT",
  "private": true,
  "main": "bin/cli.js",
  "bin": "bin/cli.js",
  "files": [
    "bin"
  ],
  "scripts": {
    "watch": "npx tsc -w -p src",
    "dev": "node .",
    "build": "npx rimraf bin && npx tsc -p src && npx license-checker --production > AUTHORS",
    "pack": "npx rimraf build && npm run build && npx pkg --out-path build --compress GZip bin/cli.js"
  },
  "devDependencies": {
    "@bizhermit/basic-utils": "^1.0.0",
    "@types/node": "^17.0.9",
    "license-checker": "^25.0.1",
    "pkg": "^5.5.2",
    "rimraf": "^3.0.2",
    "typescript": "^4.5.4"
  }
}
